
<div class="form_container">
  <div class="login_form">
  <h1 id="new_session">Log In</h1>

    <%= form_tag sessions_path do %>
    <div class="field">
    <%= label_tag :email %><br>
    <%= text_field_tag :email %>
    </div>
      <div class="field">
        <%= label_tag :password %><br>
    <%= password_field_tag :password %>
    </div>
      <div class="actions">
        <%= submit_tag "Log In" %>
    </div>
    <% end %>
  </div>

</div>
